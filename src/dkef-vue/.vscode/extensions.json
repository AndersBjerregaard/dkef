{
  "tailwindCSS": {
    "experimental.configFile": "./tailwind.config.js",
    "files.associations": {
      "*.html": "tailwindcss",
      "*.css": "tailwindcss",
      "*.vue": "tailwindcss"
    },
    "editor.quickSuggestions": {
      "strings": "on"
    }
  },
  "recommendations": [
    "Vue.volar",
    "vitest.explorer",
    "dbaeumer.vscode-eslint",
    "EditorConfig.EditorConfig",
    "esbenp.prettier-vscode"
  ]
}
